{
  "expo": {
    "name": "inStock",
    "slug": "instock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "instock",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jimothych.instock",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_small.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jimothych.instock"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash_screen.png",
          "backgroundColor": "#210340",
          "enableFullScreenImage_legacy": true,
          "resizeMode": "native"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Inter_28pt-Regular.ttf",
            "./assets/fonts/Inter_28pt-Bold.ttf",
            "./assets/fonts/Inter_28pt-Italic.ttf",
            "./assets/fonts/Inter_28pt-LightItalic.ttf",
            "./assets/fonts/Inter_28pt-BoldItalic.ttf",
            "./assets/fonts/Inter_28pt-Light.ttf"
          ]
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.366513490502-t63q7meg415m6r4r6dva3qb269crs1p3"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "dd5aab85-95ff-4f05-a118-6553212e723b"
      }
    },
    "owner": "jimothych"
  }
}
